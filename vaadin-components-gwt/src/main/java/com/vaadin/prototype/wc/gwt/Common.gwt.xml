<module>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.query.QueryMin'/>
    <inherits name='elemental.Json' />
    <inherits name='com.vaadin.Vaadin'/>
    <generate-with
        class="com.vaadin.server.widgetsetutils.ConnectorBundleLoaderFactory">
        <when-type-assignable
            class="com.vaadin.client.metadata.ConnectorBundleLoader" />
    </generate-with>
    <generate-with
        class="com.vaadin.server.widgetsetutils.AcceptCriteriaFactoryGenerator">
        <when-type-is class="com.vaadin.client.ui.dd.VAcceptCriterionFactory" />
    </generate-with>


    <add-linker name="xsiframe"/>
    <define-linker name="wclinker" class="com.vaadin.prototype.wc.WCLinker" />
    <add-linker name="wclinker" />
    <collapse-all-properties value="true" />

    <set-property name="user.agent" value="safari,gecko1_8,ie10" />
    <set-property name="compiler.stackMode" value="strip" />
    <set-property name="gwt.logging.enabled" value="FALSE"/>

    <replace-with class="com.google.gwt.query.client.impl.SelectorEngineNativeMin">
        <when-type-assignable class="com.google.gwt.query.client.impl.SelectorEngineImpl"/>
        <when-property-is name="user.agent" value="gecko1_8"/>
    </replace-with>
</module>

