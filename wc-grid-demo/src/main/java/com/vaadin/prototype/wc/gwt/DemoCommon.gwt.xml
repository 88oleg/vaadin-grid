<module rename-to='Demo'>
<!--     <inherits name='com.google.gwt.query.Query'/> -->

<!--     <inherits name='com.vaadin.Vaadin'/> -->
    <inherits name='com.vaadin.DefaultWidgetSet'/>

    <inherits name="com.google.gwt.user.theme.chrome.ChromeResources"/>
    <inherits name="com.google.gwt.user.theme.clean.CleanResources"/>
    <inherits name="com.google.gwt.user.theme.dark.DarkResources"/>
    <inherits name="com.google.gwt.user.theme.standard.StandardResources"/>

    <!-- TODO: for some reason jsinterop fails with FF and IE
         errors are casting problems seems a problem in GWT
     -->
    <set-property name="user.agent" value="safari,gecko1_8,ie10,ie9,ie8" />
    <set-property name="user.agent" value="safari" />

    <!-- downloading the JS as a list of strings and then installing it into the iframe -->
    <add-linker name="xsiframe"/>
    <!--  adds a script tag directly to the iframe -->
    <!-- <add-linker name="direct_install" /> -->

    <collapse-all-properties/>

    <stylesheet src="demo.css" />
    
    <!-- Aditionale create a Module-import.html file to use with Web Components -->
    <define-linker name="wclinker" class="com.vaadin.prototype.wc.WCLinker" />
    <add-linker name="wclinker" />
<!-- <set-property name="compiler.stackMode" value="strip" /> -->
</module>

